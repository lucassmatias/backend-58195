<div>
    {{> home}}
</div>

<h2>Paginación</h2>
{{#if data.hasPrevPage}}
    <a href="{{prevPage}}">Anterior</a>
{{/if}}
{{#if data.hasNextPage}}
    <a href="{{nextPage}}">Siguiente</a>
{{/if}}
<br></br>
<h2>Formulario para crear producto</h2>
<div class="formulario-contacto">
    <form action="">
        <p>
            <label>Codigo</label>
            <input type="text" id="code" placeholder="Codigo">
        </p>        
        <p>
            <label>Nombre</label>
            <input type="text" id="name" placeholder="Nombre">
        </p>
        <p>
            <label>Precio</label>
            <input type="text" id="price" placeholder="Precio">
        </p>
        <p>
            <label>Stock</label>
            <input type="text" id="stock" placeholder="Stock">
        </p>
        <p>
            <label>Descripción</label>
            <textarea name="Descripcion" rows="3" id="description" placeholder="Descripcion"></textarea>
        </p>
        <p>
        <button id="input">
            Enviar
        </button>
        </p>
    </form>
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="js/index.js"></script>

